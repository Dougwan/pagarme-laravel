{
    "name": "dougwn/pagarme-laravel",
    "version": "1.0.2",
    "description": "A Laravel package to integrate with Pagar.me API",
    "keywords": [
        "laravel",
        "pagarme"
    ],
    "homepage": "https://github.com/keepcloud/",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Louteiro",
            "email": "dgwnlnx@protonmail.ch"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.7",
        "illuminate/contracts": "^11.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "autoload": {
        "psr-4": {
            "Dougwn\\Pagarme\\": "src/",
            "Dougwn\\Pagarme\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dougwn\\Pagarme\\PagarmeServiceProvider"
            ],
            "aliases": {
                "Pagarme": "Dougwn\\Pagarme\\Facades\\Pagarme"
            }
        }
    },
    "minimum-stability": "dev"
}
